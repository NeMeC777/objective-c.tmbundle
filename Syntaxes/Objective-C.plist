{
	name = "Objective-C";
	fileTypes = ( m, h );
	foldingStartMarker = "(/\\*\\*|\\{\\s*$)";
	foldingStopMarker = "(\\*\\*/|^\\s*\\})";
	increaseIndentPattern = "^.*(\\{[^}\"']*|\\([^)\"']*)$";
	smartTypingPairs = (
	   ( "(", ")" ), ( "[", "]" ), ( "{", "}" ),
	   ( "\"", "\"" ), ( "'", "'" )
	);
	highlightPairs = (
	   ( "(", ")" ), ( "[", "]" ), ( "{", "}" )
	);
	foregroundColor = "#000000";
	backgroundColor = "#FFFFFF";
	patterns = (
	   { name = "declaration.function.objc";
	     begin = "^(-|\\+)\\s*"; end = "\\{|;"; swallow = "(//.*$)";
	     patterns = (
     		{ begin = "/\\*"; end = "\\*/"; foregroundColor = "#236B8E"; },
	      { match = "//.*$"; foregroundColor = "#236B8E"; fontStyle = ( italic ); },
	      { match = ":\\s*(\\([^)]*\\))\\s*(\\<\\w+\\>)?";
	        "foregroundColor[1]" = "#555555"; fontStyle = ( plain ); },
         { match = "(\\([^)]*\\))\\s*(\\<\\w+\\>)?";
           "foregroundColor[1]" = "#555555"; "fontStyle[2]" = ( bold ); },
         { match = "\\<\\w+\\>(?=:)"; fontStyle = ( bold ); },
	     );
	   },
		{ name = "strings.double-quoted.objc";
		  begin = "@\""; end = "\""; swallow = "\\\\.";
		  foregroundColor = "#224d14"; },
		{ name = "keywords.objc";
		  match = "\\<(id|BOOL|SEL|IBOutlet|IBAction|self|super|in|out|inout|oneway|YES|NO|nil)\\>";
		  foregroundColor = "blue";
		  fontStyle = ( bold ); },
		{ name = "keywords.objc.directives";
		  match = "@(class|interface|implementation|synchronized|protocol|selector|defs|try|catch|finally|throw|end)\\>";
		  "fontStyle[1]" = ( bold ); foregroundColor = "blue"; },
		{ include = "C"; },
	);
	uuid = "F85CC716-6B1C-11D9-9A20-000D93589AF6";
}
