                        1 = { name = "storage.type.method.result.objc"; };
                        2 = { name = "entity.name.function.objc"; };
                    };
                },
                {   name = "keyword.other.name-of-parameter.objc"; match = "\\b\\w+\\b(?=:)"; },
            );
        },
		{   name = "string.quoted.double.objc";
		    begin = "@\""; end = "\""; swallow = "\\\\.";
		},
		{   name = "keyword.control.macro.objc";
		    match = "\\b(NS_DURING|NS_HANDLER|NS_ENDHANDLER)\\b";
		},
		{   name = "declaration.class.objc";
		    match = "(@(interface|implementation))\\s+([A-Za-z]+)\\s*(?::\\s*([A-Za-z]+))?";
		    captures = {
		        1 = { name = "storage.type.objc"; };
		        3 = { name = "entity.name.class.objc"; };
		        4 = { name = "entity.other.inherited-class"; };
		    };
		},

        {   name = "keyword.control.objc"; match = "@(try|catch|finally|throw)\\b"; },
        {   name = "keyword.other.objc"; match = "@(defs|encode)\\b"; },
        {   name = "storage.type.objc"; match = "\\b(IBOutlet|IBAction|BOOL|SEL|id)\\b"; },
        {   name = "storage.type.objc"; match = "@(class|protocol|end|selector)\\b"; },
        {   name = "storage.modifier.objc"; match = "@(synchronized|public|private|protected)\\b"; },
        {   name = "storage.modifier.objc"; match = "\\b(in|out|inout|oneway)\\b"; },
